<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         metadata-complete="false"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginDeal</servlet-name>
        <servlet-class>top.lking.servlets.LoginDealServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginDeal</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DelByAjaxServlet</servlet-name>
        <servlet-class>top.lking.servlets.ajax.DelLQUserAJAXServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DelByAjaxServlet</servlet-name>
        <url-pattern>/delbyajax.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>top.lking.servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
</web-app>